{
    "name": "neeckeloo/wizard",
    "description": "Wizard module for Zend Framework 2",
    "type": "library",
    "keywords": [
        "zf2", "wizard"
    ],
    "license": "MIT",
    "homepage": "https://github.com/neeckeloo/Wizard",
    "authors": [
        {
            "name": "Nicolas Eeckeloo",
            "email": "neeckeloo@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/options-resolver": "^2.6 || ^3.0",
        "container-interop/container-interop": "^1.1.0",
        "zendframework/zend-form": "^2.7",
        "zendframework/zend-eventmanager": "^3.0",
        "zendframework/zend-filter": "^2.6",
        "zendframework/zend-http": "^2.5.4",
        "zendframework/zend-modulemanager": "^2.7.1",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-router": "^3.0.1",
        "zendframework/zend-servicemanager": "^3.0.3",
        "zendframework/zend-session": "^2.6.0",
        "zendframework/zend-stdlib": "^3.0",
        "zendframework/zend-view": "^2.6.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Wizard\\": "src/Wizard/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WizardTest\\": "tests/WizardTest/"
        }
    }
}
